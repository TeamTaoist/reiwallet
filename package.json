    "uuid": "^8.3.2",
    "webextension-polyfill": "^0.10.0"
  },
  "overrides": {
    "@ckb-lumos/ckb-indexer": {
      "cross-fetch": "^4.0.0"
    },
    "@ckb-lumos/lumos": {
      "cross-fetch": "^4.0.0"
    },
    "@ckb-lumos/rpc": {
      "cross-fetch": "^4.0.0"
    }
  }
}
