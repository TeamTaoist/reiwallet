    "postcss-preset-env": "^7.0.1",
    "prompts": "^2.4.2",
    "react": "^18.1.0",
    "react-app-polyfill": "^3.0.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.1.0",
    "react-qr-code": "^2.0.12",
