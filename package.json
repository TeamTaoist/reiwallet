{
  "name": "reiwallet",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "start": "node ./scripts/start.js",
    "postbuild": "node ./scripts/postbuild.js",
    "build": "node ./scripts//build.js",
    "test": "echo \"No test specified\"",
    "watch": "npm-watch",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "watch": {
    "build": {
      "patterns": [
        ".",
        "src/**",
        "public/**"
      ],
      "ignore": [
        "build",
        "node_modules"
      ],
      "extensions": [
        "js",
        "json",
        "lock",
        "css",
        "jsx"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "default-case": "off",
      "no-lone-blocks": "off",
      "react-hooks/exhaustive-deps": "off",
      "no-redeclare": "off",
      "array-callback-return": "off",
      "import/no-anonymous-default-export": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "dependencies": {
    "@ckb-lumos/base": "^0.22.0-next.5",
    "@ckb-lumos/bi": "^0.22.0-next.5",
    "@ckb-lumos/ckb-indexer": "^0.22.0-next.5",
    "@ckb-lumos/codec": "^0.22.0-next.5",
    "@ckb-lumos/common-scripts": "^0.22.0-next.5",
    "@ckb-lumos/config-manager": "^0.22.0-next.5",
    "@ckb-lumos/hd": "^0.22.0-next.5",
    "@ckb-lumos/helpers": "^0.22.0-next.5",
    "@ckb-lumos/lumos": "^0.22.0-next.5",
    "@ckb-lumos/rpc": "^0.22.0-next.5",
    "@nervina-labs/ckb-dex": "^0.2.10",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@rgbpp-sdk/btc": "^0.6.0",
    "@rgbpp-sdk/ckb": "^0.6.0",
    "@rgbpp-sdk/service": "^0.6.0",
    "@sentry/browser": "^8.31.0",
    "@spore-sdk/core": "0.2.0",
    "@svgr/webpack": "^5.5.0",
    "@taoist-labs/dob-decoder": "^0.0.14",
    "axios": "^1.7.7",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "bip39": "^3.1.0",
